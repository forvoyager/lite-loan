      </if>
    </trim>
    <where>
      <if test="user_id != null">
        <![CDATA[ AND t.`user_id` = #{user_id} ]]>
      </if>
      <if test="where_version != null">
        <![CDATA[ AND t.`version` = #{where_version} ]]>
      </if>
    </where>
  </update>

