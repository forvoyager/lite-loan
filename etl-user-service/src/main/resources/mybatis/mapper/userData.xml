        <![CDATA[ `version`, ]]>
      </if>
    </trim>
    <![CDATA[ VALUES ]]>
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="user_id != null">
        <![CDATA[ #{user_id}, ]]>
