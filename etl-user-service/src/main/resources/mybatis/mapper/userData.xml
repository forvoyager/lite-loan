    <if test="update_time != null">
      <![CDATA[ AND t.`update_time` = #{update_time} ]]>
    </if>
  </sql>

  <sql id="Where_Sql">
    <where>
      <include refid="Base_Where_Sql"/>
      <include refid="Custom_Where_Sql"/>
    </where>
  </sql>

  <!-- 插入 -->
  <insert id="insert" parameterType="com.etl.user.common.model.UserDataModel" keyProperty="user_id">
    <![CDATA[ INSERT INTO `etl_user_data` ]]>
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="user_id != null">
        <![CDATA[ `user_id`, ]]>
      </if>
      <if test="gender != null">
        <![CDATA[ `gender`, ]]>
