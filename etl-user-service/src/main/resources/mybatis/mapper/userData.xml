      </if>
    </trim>
  </insert>

  <!-- 批量插入 -->
  <insert id="insertBatch" parameterType="com.etl.user.common.model.UserDataModel">
    <![CDATA[ INSERT INTO `etl_user_data` (`user_id`, `gender`, `id_name`, `id_number`, `contacts`, `jobs`, `residence`, `create_time`, `update_time`, `version`) VALUES ]]>
    <foreach collection="list" separator="," item="item">
      (#{item.user_id}, #{item.gender}, #{item.id_name}, #{item.id_number}, #{item.contacts}, #{item.jobs}, #{item.residence}, #{item.create_time}, #{item.update_time}, #{item.version})
    </foreach>
