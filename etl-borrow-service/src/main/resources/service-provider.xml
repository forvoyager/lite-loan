<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

  <dubbo:application name="${spring.application.name}_provider"/>
  <dubbo:registry address="zookeeper://127.0.0.1:2181" client="curator"/>
  <dubbo:protocol name="hessian" server="servlet" port="${server.port}"/>
  <!--<dubbo:protocol name="dubbo" server="netty" port="11001"/>-->

  <!--export service-->
  <dubbo:service ref="borrowService" interface="com.etl.borrow.common.service.IBorrowService" version="${borrow.service.version}" group="${borrow.service.group}" filter="remoteServiceFilter" retries="0" />
  <dubbo:service ref="repaymentFormService" interface="com.etl.borrow.common.service.IRepaymentFormService" version="${borrow.service.version}" group="${borrow.service.group}" filter="remoteServiceFilter" retries="0" />

</beans>
