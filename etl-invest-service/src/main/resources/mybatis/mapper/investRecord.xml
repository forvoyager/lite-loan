<!-- description: 投资记录 数据库操作mapper -->
<mapper namespace="com.etl.invest.mapper.InvestRecordMapper">

  <sql id="Base_Column_List">
    `invest_id`, `user_id`, `borrow_id`, `invest_amount`, `partion`, `status`, `channel`, `create_time`, `update_time`, `version`
  </sql>

  <!-- 自定义查询条件 -->
  <sql id="Custom_Where_Sql">

  </sql>

  <!-- 基础查询条件，不允许修改，自定义条件在Custom_Where_Sql中添加 -->
  <sql id="Base_Where_Sql">
    <if test="idList != null and idList.size() > 0">
      <![CDATA[ AND t.`invest_id` IN ]]>
      <foreach collection="idList" item="item" open="(" separator="," close=")">
        <![CDATA[ #{item} ]]>
      </foreach>
    </if>
    <if test="where_version != null">
      <![CDATA[ AND t.`version` = #{where_version} ]]>
    </if>
    <if test="invest_id != null">
      <![CDATA[ AND t.`invest_id` = #{invest_id} ]]>
    </if>
    <if test="user_id != null">
      <![CDATA[ AND t.`user_id` = #{user_id} ]]>
    </if>
    <if test="borrow_id != null">
      <![CDATA[ AND t.`borrow_id` = #{borrow_id} ]]>
    </if>
    <if test="invest_amount != null">
      <![CDATA[ AND t.`invest_amount` = #{invest_amount} ]]>
    </if>
    <if test="partion != null">
      <![CDATA[ AND t.`partion` = #{partion} ]]>
    </if>
    <if test="status != null">
      <![CDATA[ AND t.`status` = #{status} ]]>
    </if>
    <if test="channel != null">
      <![CDATA[ AND t.`channel` = #{channel} ]]>
    </if>
    <if test="create_time != null">
      <![CDATA[ AND t.`create_time` = #{create_time} ]]>
    </if>
    <if test="update_time != null">
      <![CDATA[ AND t.`update_time` = #{update_time} ]]>
    </if>
  </sql>

  <sql id="Where_Sql">
    <where>
      <include refid="Base_Where_Sql"/>
      <include refid="Custom_Where_Sql"/>
    </where>
  </sql>

  <!-- 插入 -->
  <insert id="insert" parameterType="com.etl.invest.common.model.InvestRecordModel" keyProperty="invest_id" useGeneratedKeys="true">
    <![CDATA[ INSERT INTO etl_invest_record ]]>
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="invest_id != null">
        <![CDATA[ `invest_id`, ]]>
      </if>
      <if test="user_id != null">
        <![CDATA[ `user_id`, ]]>
      </if>
      <if test="borrow_id != null">
        <![CDATA[ `borrow_id`, ]]>
      </if>
      <if test="invest_amount != null">
        <![CDATA[ `invest_amount`, ]]>
      </if>
      <if test="partion != null">
        <![CDATA[ `partion`, ]]>
      </if>
      <if test="status != null">
        <![CDATA[ `status`, ]]>
      </if>
      <if test="channel != null">
        <![CDATA[ `channel`, ]]>
      </if>
      <if test="create_time != null">
        <![CDATA[ `create_time`, ]]>
      </if>
      <if test="update_time != null">
        <![CDATA[ `update_time`, ]]>
      </if>
      <if test="version != null">
        <![CDATA[ `version`, ]]>
      </if>
    </trim>
    <![CDATA[ VALUES ]]>
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="invest_id != null">
        <![CDATA[ #{invest_id}, ]]>
      </if>
      <if test="user_id != null">
        <![CDATA[ #{user_id}, ]]>
      </if>
      <if test="borrow_id != null">
        <![CDATA[ #{borrow_id}, ]]>
      </if>
      <if test="invest_amount != null">
        <![CDATA[ #{invest_amount}, ]]>
      </if>
      <if test="partion != null">
        <![CDATA[ #{partion}, ]]>
      </if>
      <if test="status != null">
        <![CDATA[ #{status}, ]]>
      </if>
      <if test="channel != null">
        <![CDATA[ #{channel}, ]]>
      </if>
      <if test="create_time != null">
        <![CDATA[ #{create_time}, ]]>
      </if>
      <if test="update_time != null">
        <![CDATA[ #{update_time}, ]]>
      </if>
      <if test="version != null">
        <![CDATA[ #{version}, ]]>
      </if>
    </trim>
  </insert>

  <!-- 批量插入 -->
  <insert id="insertBatch" parameterType="com.etl.invest.common.model.InvestRecordModel">
    <![CDATA[ INSERT INTO etl_invest_record (`invest_id`, `user_id`, `borrow_id`, `invest_amount`, `partion`, `status`, `channel`, `create_time`, `update_time`, `version`) VALUES ]]>
    <foreach collection="list" separator="," item="item">
      (#{item.invest_id}, #{item.user_id}, #{item.borrow_id}, #{item.invest_amount}, #{item.partion}, #{item.status}, #{item.channel}, #{item.create_time}, #{item.update_time}, #{item.version})
    </foreach>
  </insert>

  <!-- 更新（主键/version/create_time不会被更新，version自增1） -->
  <update id="update" parameterType="map">
    <![CDATA[ UPDATE etl_invest_record t SET t.`version` = t.`version` + 1, ]]>
    <trim suffixOverrides=",">
      <if test="user_id != null">
        <![CDATA[ t.`user_id` = #{user_id}, ]]>
      </if>
      <if test="borrow_id != null">
        <![CDATA[ t.`borrow_id` = #{borrow_id}, ]]>
      </if>
      <if test="invest_amount != null">
        <![CDATA[ t.`invest_amount` = #{invest_amount}, ]]>
      </if>
      <if test="partion != null">
        <![CDATA[ t.`partion` = #{partion}, ]]>
      </if>
      <if test="status != null">
        <![CDATA[ t.`status` = #{status}, ]]>
      </if>
      <if test="channel != null">
        <![CDATA[ t.`channel` = #{channel}, ]]>
      </if>
      <if test="update_time != null">
        <![CDATA[ t.`update_time` = #{update_time}, ]]>
      </if>
    </trim>
    <where>
      <if test="invest_id != null">
        <![CDATA[ AND t.`invest_id` = #{invest_id} ]]>
      </if>
      <if test="where_version != null">
        <![CDATA[ AND t.`version` = #{where_version} ]]>
      </if>
    </where>
  </update>

  <!-- 删除 -->
  <delete id="delete" parameterType="map">
    <![CDATA[ DELETE t FROM etl_invest_record t ]]>
    <include refid="Where_Sql"/>
  </delete>

  <!-- 查询行数 -->
  <select id="selectCount" parameterType="map" resultType="long">
    <![CDATA[ SELECT COUNT(*) FROM etl_invest_record t ]]>
    <include refid="Where_Sql"/>
  </select>

  <!-- 查询记录 -->
  <select id="selectList" parameterType="map" resultType="com.etl.invest.common.model.InvestRecordModel">
    <include refid="global.pageStart"/>
    <![CDATA[ SELECT ]]>
    <include refid="Base_Column_List"/>
    <![CDATA[ FROM etl_invest_record t ]]>
    <include refid="Where_Sql"/>
    <include refid="global.globalSort"/>
    <include refid="global.pageEnd"/>
  </select>

</mapper>