<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--<dubbo:application name="${spring.application.name}"/>-->
    <dubbo:registry address="zookeeper://127.0.0.1:2181" client="curator"/>

    <!--borrow application-->
    <dubbo:reference id="borrowService" interface="com.etl.asset.common.service.IBorrowService" version="${borrow.service.version}" group="${borrow.service.group}" check="false" />
    <dubbo:reference id="repaymentFormService" interface="com.etl.asset.common.service.IRepaymentFormService" version="${borrow.service.version}" group="${borrow.service.group}" check="false" />

    <!--user application-->
    <dubbo:reference id="userAccountService" interface="com.etl.user.common.service.IUserAccountService" version="${user.service.version}" group="${user.service.group}" check="false" />
    <dubbo:reference id="userAccountBiz" interface="com.etl.user.common.service.IUserAccountBiz" version="${user.service.version}" group="${user.service.group}" check="false" />

</beans>
